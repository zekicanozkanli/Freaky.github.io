<script src="https://cdn.jsdelivr.net/npm/typed.js@2.0.12"></script>

<style>
span { color: #3FD824 !important; text-align: center; font-size: 30px; }
* { max-width: 100vw; max-height: 100vh; }
</style>

<div id="typed-strings">
  <p><strong>Mix Engineer</strong></p>
  <p><em></em><strong>Mathematician<strong></p>
</div>
<span id="typed"></span>

<script>
  var typed = new Typed('#typed', {
    stringsElement: '#typed-strings',
    typeSpeed: 50,
    backSpeed: 50,
    loop: true,
    backDelay: 850
  });
</script>

